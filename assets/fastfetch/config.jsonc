{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"logo": {
		"type": "file-raw",
		"source": "~/.config/fastfetch/dotmiku",
		"padding": {
			"right": 1
		}
	},
	"display": {
		"separator": "",
		"color": {
			"keys": "yellow"
		}
	},
	"modules": [
		{
			"type": "title",
			"format": "{#1;32}{user-name}@{host-name}{#}"
		}, {
			"type": "separator"
		}, {
			"type": "datetime",
			"key": " "
		}, {
			"type": "break"
		}, {
			"type": "os",
			"format": "{pretty-name} {codename}",
			"keyWidth": 8
		}, {
			"type": "kernel",
			"format": "{sysname}-{arch} {release}",
			"keyWidth": 8
		}, {
			"type": "bios",
			"format": "{release}/{version} {type}",
			"key": "BIOS",
			"keyWidth": 8
		}, {
			"type": "initsystem",
			"format": "{name}({4}) {version}",
			"key": "Init",
			"keyWidth": 8
		}, {
			"type": "uptime",
			"keyWidth": 8
		}, {
			"type": "break"
		}, {
			"type": "board",
			"format": "{vendor:8} {name}",
			"keyWidth": 7
		}, {
			"type": "cpu",
			"format": "{name}",
			"keyWidth": 7
		}, {
			"type": "gpu",
			"format": "{vendor} {name}",
			"keyWidth": 7
		}, {
			"type": "memory",
			"format": "{percentage-bar} of {total}",
			"keyWidth": 16
		}, {
			"type": "disk",
			"format": "{size-percentage-bar} of {size-total}",
			"keyWidth": 16
		}, {
			"type": "break"
		}, {
			"type": "monitor",
			"format": "{width}x{height}{#37}@{#}{refresh-rate}Hz",
			"keyWidth": 24
		}, {
			"type": "break"
		}, {
			"type": "shell",
			"format": "{pretty-name} v{version:3}",
			"keyWidth": 8
		}, {
			"type": "locale",
			"keyWidth": 8
		}, {
			"type": "break"
		}, {
			"type": "loadavg",
			"keyWidth": 11
		}, {
			"type": "processes",
			"keyWidth": 11
		}, {
			"type": "break"
		}, {
			"type": "player",
			"key": "Player",
			"keyWidth": 9
		}, {
			"type": "media",
			"format": "{combined} - {artist}",
			"key": "Playing",
			"keyWidth": 9
		}, {
			"type": "break"
		}, {
			"type": "colors"
		}
	]
}
