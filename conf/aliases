add	git add
amend	git commit --amend
branch	git branch
commit	git commit
log	git log
merge	git merge
push	git push
rebase	git rebase
show	git show
stash	git stash
switch	git switch

linel	wc -l $(git ls-files)
lines	cat $(git ls-files) | wc -l

m	make
g	gdb

useidf	source $HOME/source/esp-idf/export.sh
